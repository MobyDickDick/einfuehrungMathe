\begin_layout Solution
Übungsaufgabe 1.4 a 
\begin_inset Formula 
\[
d=\frac{m}{V}=\frac{374.5\,\text{g}}{41.8\,\text{cm}^{3}}=8.96\cdot\frac{10^{-3}\text{kg}}{10^{-6}\text{m}^{3}}=8.96\cdot10^{3}\frac{\text{kg}}{\text{\ensuremath{\text{m}}}^{3}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Solution
Übungsaufgabe 1.4 b
\end_layout

\begin_layout Solution
\begin_inset Formula 
\[
d=\frac{m}{V}
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
V=\frac{m}{d}=\frac{15.0\,\text{g}}{0.789\,\text{\ensuremath{\frac{\text{g}}{\text{ml}}}}}=19.0\,\text{ml}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Solution
Übungsaufgabe 1.4 c
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
d=\frac{m}{V}
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
m=V\cdot d=25.0\,\text{ml}\cdot13.6\,\frac{\text{g}}{\text{ml}}=340\,\text{g}
\]

\end_inset


\end_layout

\begin_layout Solution
Übungsaufgabe 1.5
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
1\,\text{cal} & =4.184\,\text{J}\\
547\,\text{kcal} & =547\cdot10^{3}\cdot4.184\,\text{J}=2.28\text{MJ}
\end{align*}

\end_inset


\end_layout

\begin_layout Solution
Übungsaufgabe 1.9
\end_layout

\begin_layout Solution
\begin_inset Formula 
\begin{align*}
v & =\frac{\triangle s}{\triangle t}=\frac{100\,\text{\ensuremath{\text{m}}}}{10.5\,\text{\ensuremath{\text{s}}}}\\
 & \doteq9.52\,\text{\ensuremath{\frac{\text{m}}{\text{s}}}}\\
 & =9.52\,\frac{\text{km}/1000}{\text{s}/3600}\\
 & =9.52\cdot\frac{3600}{1000}\cdot\frac{\text{km}}{\text{s}}\\
 & =34.3\frac{\text{km}}{\text{s}}
\end{align*}

\end_inset


\end_layout

\begin_layout Solution
Nebenbemerkung:
 Das ist ein Affenzahl,
 welcher der Läufer da hinlegt!
\end_layout

