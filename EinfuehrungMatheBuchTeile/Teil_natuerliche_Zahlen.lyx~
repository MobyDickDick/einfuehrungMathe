\begin_layout Standard
sowie 
\begin_inset Formula 
\[
\begin{array}{cccccccccc}
\text{W} & \text{W} & \text{W} & \text{W} & \text{W} & \text{W} & \text{W} & \text{W} & \text{W} & \text{W}\\
\text{W} & \text{W} & \text{W} & \text{W} & \text{W} & \text{W} & \text{W} & \text{W} & \text{W} & \text{W}\\
