 &  & \left.\phantom{\bigm|}m_{1}\in M_{1}\wedge m_{2}\in M_{2}\Rightarrow\left(m_{1},m_{2}\right)\equiv\left\{ \left\{ m_{1}\right\} ,\left\{ m_{1},m_{2}\right\} \right\} \right\} 
\end{eqnarray*}

\end_inset


\end_layout

