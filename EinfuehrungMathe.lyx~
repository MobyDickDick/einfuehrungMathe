reference "tab:2.Teil-1.-Beweis-2.konjunktive-Normalform-Implikation"
nolink "false"

\end_inset

,
 
\begin_inset CommandInset ref
LatexCommand ref
reference "tab:1.Teil-2.-Beweis-2.konjunktive-Normalform-Implikation"
nolink "false"

\end_inset

,
 
\begin_inset CommandInset ref
LatexCommand ref
reference "tab:2.Teil-2.-Beweis-2.konjunktive-Normalform-Implikation"
nolink "false"

\end_inset

